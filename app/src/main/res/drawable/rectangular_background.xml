<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <!-- Background color of the oval -->
            <solid android:color="#A84545" />

            <!-- Border of the oval -->
            <stroke
                android:width="2dp"
                android:color="#000000" />

            <!-- Size of the oval -->
            <size
                android:width="10dp"
                android:height="10dp" />
        </shape>
    </item>

</layer-list>


